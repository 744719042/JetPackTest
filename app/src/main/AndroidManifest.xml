<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sohu.jetpacktest">

    <application
        android:name=".app.JetPackApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RoomActivity" />
        <activity android:name=".LifecycleTestActivity" />
        <activity android:name=".AppCompatTestActivity" />
        <activity android:name=".CommonActivityTestActivity" />
        <activity
            android:name=".DialogActivity"
            android:theme="@style/Theme.ActivityDialogStyle" />
        <activity
            android:name=".ChangeDialogActivity"
            android:theme="@style/Theme.ActivityDialogStyle" />
        <activity android:name=".ViewModelTestActivity" />
        <activity android:name=".ViewModelRotateActivity" />
        <activity android:name=".ViewModelCommunicateActivity" />
        <activity android:name=".LiveDataTestActivity" />
        <activity android:name=".BindingTestActivity" />
        <activity android:name=".PagingTestActivity" />
        <activity android:name=".NavigationTestActivity" />
        <activity android:name=".MovieDemoActivity"></activity>
    </application>

</manifest>